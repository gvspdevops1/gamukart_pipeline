pipeline {
  agent any
  stages {
    stage("Checkout") {
      steps {
        sh 'echo checkout done '
      }
    }
    stage("build ") {
      steps {     
        sh 'mvn clean install'
      }
    }
    stage("stage3") {
      steps {     
        sh 'echo stage 3 completed '
      }
    }
    stage("stage4 ") {
      steps {     
        sh 'echo stage 4 completed '
      }
    }
    stage("stage5 ") {
      steps {     
        sh 'echo stage 5 completed '
      }
    }
    stage("stage6 ") {
      steps {     
        sh 'echo stage 6 completed '
      }
    }
    //stage("terraform") {
      //steps {         
	//sh 'terraform init'      
        //sh 'terraform apply -auto-approve'
      //}
    //}
    stage("Build Image") {
      steps {     
	sh 'whoami'      
        sh 'docker build -t gvsiva2008/jenkins_july:v1.2 .'
      }
    }
    stage("pushtoHub") { 
        steps{
           withCredentials([usernamePassword(credentialsId: 'dockerHub', passwordVariable: 'dockerHubPassword', usernameVariable: 'dockerHubUser')]) {
             sh "docker login -u ${env.dockerHubUser} -p ${env.dockerHubPassword}"
             sh 'docker push gvsiva2008/jenkins_july:v1.2'
           }
        }
     }
  }
}
